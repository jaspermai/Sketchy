{
    "name": "Sketchy",
    "version": "1.0.0",
    "description": "Sketch it now!",
    "permissions": ["tabs","activeTab","declarativeContent", "storage"],
    "page_action": {
      "default_popup": "popup.html"
    },
    "background": 
    {
        "scripts": ["background.js"]
    },
    "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "css": ["popup.css"],
          "js": ["content.js"]
      }],
    "manifest_version": 2,
    "icons": {
      "16": "./images/ICON.png",
      "48": "./images/ICON.png",
      "128": "./images/ICON.png"
    }
  }
